  "imports": {
    "@dreamer/view": "./src/mod.ts",
    "@dreamer/view/effect": "./src/effect.ts",
    "@dreamer/view/dom": "./src/dom.ts",
    "@dreamer/view/dom/element": "./src/dom/element.ts",
    "@dreamer/view/dom/unmount": "./src/dom/unmount.ts",
    "@dreamer/view/dom/hydrate": "./src/dom/hydrate.ts",
    "@dreamer/view/types": "./src/types.ts",
    "@dreamer/view/dev": "./src/dev.ts",
    "@dreamer/view/jsx-runtime": "./src/jsx-runtime.ts",
    "@dreamer/view/store": "./src/store.ts",
    "@dreamer/view/reactive": "./src/reactive.ts",
    "@dreamer/view/router": "./src/router.ts",
    "@dreamer/test": "jsr:@dreamer/test@^1.0.5",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.5",
    "@dreamer/console": "jsr:@dreamer/console@^1.0.6",
    "@dreamer/esbuild": "jsr:@dreamer/esbuild@^1.0.9",
    "@dreamer/server": "jsr:@dreamer/server@^1.0.5",
    "@dreamer/middleware": "jsr:@dreamer/middleware@^1.0.0",
    "@dreamer/router": "jsr:@dreamer/router@^1.0.8",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.0",
    "@dreamer/render": "jsr:@dreamer/render@^1.0.13",
    "happy-dom": "npm:happy-dom@20.4.0"
  },
  "compilerOptions": {
    "jsx": "react",
    "lib": [
      "deno.window",
      "dom"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "exclude": [
    "dist/*",
    "examples/*",
    "tests/data/",
    "jsx.d.ts"
  ],
  "publish": {
    "exclude": [
      "jsx.d.ts"
    ]
  },
  "nodeModulesDir": "auto",
  "tasks": {
    "test": "deno test -A tests/",
    "build": "deno run -A build.ts"
  }
}
