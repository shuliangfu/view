  ],
  "exports": {
    ".": "./src/mod.ts",
    "./cli": "./src/cli.ts",
    "./setup": "./src/setup.ts",
    "./csr": "./src/mod-csr.ts",
    "./hybrid": "./src/mod-hybrid.ts",
    "./jsx-runtime": "./src/jsx-runtime.ts",
    "./reactive": "./src/reactive.ts",
    "./boundary": "./src/boundary.ts",
    "./directive": "./src/directive.ts",
    "./resource": "./src/resource.ts",
    "./compiler": "./src/compiler.ts",
    "./context": "./src/context.ts",
    "./stream": "./src/stream.ts",
    "./router": "./src/router.ts",
    "./store": "./src/store.ts",
    "./types": "./src/types.ts",
    "./signal": "./src/signal.ts",
    "./effect": "./src/effect.ts",
    "./scheduler": "./src/scheduler.ts",
    "./proxy": "./src/proxy.ts",
    "./meta": "./src/meta.ts",
    "./route-page": "./src/route-page.tsx",
    "./hmr": "./src/hmr.ts",
    "./dom": "./src/dom.ts",
    "./dom/shared": "./src/dom/shared.ts",
    "./dom/unmount": "./src/dom/unmount.ts",
    "./dom/element": "./src/dom/element.ts",
    "./dom/stringify": "./src/dom/stringify.ts",
    "./dom/hydrate": "./src/dom/hydrate.ts",
    "./dom/props": "./src/dom/props.ts"
